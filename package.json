{
  "name": "rezap",
  "version": "1.0.0",
  "description": "replacement for zapier",
  "main": "src/index.js",
  "author": "Michael Mroczka",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "format": "prettier --write \"src/**/*.{js,js}\"",
    "start": "node ./dist",
    "dev": "babel-node -x \".ts,.tsx,.js\" src/index.ts",
    "build": "babel -x \".ts,.tsx,.js\" ./src --out-dir ./dist",
    "typecheck": "tsc",
    "install": "yarn build"
  },
  "dependencies": {
    "@notionhq/client": "^0.1.9",
    "axios": "^0.21.1",
    "bree": "^6.2.0",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "googleapis": "^39.2.0",
    "mongodb": "^3.6.9",
    "mongoose": "^5.12.14",
    "node-graceful": "^3.0.1",
    "nodemailer": "^6.7.3",
    "pino": "^6.12.0",
    "simple-node-logger": "^18.12.24",
    "twilio": "^3.76.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bree": "^6.2.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.4",
    "@types/pino": "^6.3.9",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "prettier": "2.3.1",
    "typescript": "^4.3.4"
  }
}
